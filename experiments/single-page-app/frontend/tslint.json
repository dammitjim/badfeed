{
    "defaultSeverity": "warning",
    "extends": ["tslint:recommended"],
    "linterOptions": {
        "exclude": ["node_modules/**"]
    },
    "rules": {
        "quotemark": [true, "double"],
        "indent": [true, "spaces", 4],
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "trailing-comma": true,
        "arrow-parens": false
    }
}
